{
   "env": {
     "browser": true,
     "es6": true
   },

  "extends": ["plugin:react/recommended", "google"],
   "globals": {
     "Atomics": "readonly",
     "SharedArrayBuffer": "readonly"
   },
   "parserOptions": {
     "ecmaVersion": 6,
     "ecmaFeatures": {
       "jsx": true
     },

     "sourceType": "module"
   },
   "plugins": ["react", "unused-imports"],

   "rules": {
     "semi": "error",
     "no-unused-vars": "off",
     "unused-imports/no-unused-imports": 2,
     "unused-imports/no-unused-vars": 1

   }
}
