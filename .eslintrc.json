{
   "env": {
     "browser": true,
     "node": true
   },
   "extends": ["google"],
   "globals": {
     "Atomics": "readonly",
     "SharedArrayBuffer": "readonly"
   },
   "parserOptions": {

     "ecmaVersion": 2015,
     "sourceType": "module"
   },
   "plugins": ["react", "unused-imports"],
   "rules": {
     "no-unused-vars": "off",
     "unused-imports/no-unused-imports": 2,
     "unused-imports/no-unused-vars": 1
   }
}
