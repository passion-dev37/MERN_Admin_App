    "bootstrap": "^4.4.1",
    "concurrently": "^5.0.2",
    "config": "^3.2.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.1",
    "qrcode": "^1.4.4",
    "react-transition-group": "^4.3.0",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.2",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
